  <script>
    const celInput = document.getElementById("cel");

    celInput.addEventListener("input", function() {
      const inputText = celInput.value.replace(/\D/g, "").substring(0,11);
      const formattedText = `(${inputText.slice(0,2)})${inputText.slice(2,7)}-${inputText.slice(7)}`;
      celInput.value = formattedText;
    });
  </script>

