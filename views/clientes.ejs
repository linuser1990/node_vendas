<%- include('./header'); %>

<table class="styled-table">
    <thead>
        <tr>
            <th>Código</th>
            <th>Nome</th>
            <th>Celular</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
    
	<%
		var p = pessoas.length;
		for(var i=0;i<p;i++)
		{
	%>
            <tr>
                <td><%=pessoas[i].codigo %></td>
                <td><%=pessoas[i].nome %></td>
                <td><%=pessoas[i].cel %></td>
                <td><a href="/deletar/<%= pessoas[i].codigo %>"> Deletar </a> - 
                    <a href="/editar/<%= pessoas[i].codigo %>" > Alterar </a> 
                
                </td>
               
            </tr>
			
	<%
		}	
	%>
        <!-- and so on... -->
    </tbody>
</table>


	

<%- include('./footer'); %>